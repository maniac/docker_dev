cmake -DCMAKE_TOOLCHAIN_FILE=/cmake.x86_64-w64-mingw32 -DCMAKE_INSTALL_PREFIX=/mingw/x86_64-w64-mingw32 -DCMAKE_SYSTEM_PROCESSOR=x86_64 -DCMAKE_EXE_LINKER_FLAGS="-static-libgcc -static-libstdc++" -DCMAKE_SHARED_LINKER_FLAGS="-static-libgcc -static-libstdc++" -DCMAKE_C_FLAGS="-D_WIN32_WINNT=_WIN32_WINNT_WIN7" -DCMAKE_CXX_FLAGS="-D_WIN32_WINNT=_WIN32_WINNT_WIN7" $*

