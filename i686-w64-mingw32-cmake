cmake -DCMAKE_TOOLCHAIN_FILE=/cmake.i686-w64-mingw32 -DCMAKE_INSTALL_PREFIX=/mingw/i686-w64-mingw32 -DCMAKE_SYSTEM_PROCESSOR=i386 -DCMAKE_EXE_LINKER_FLAGS="-static-libgcc -static-libstdc++" -DCMAKE_SHARED_LINKER_FLAGS="-static-libgcc -static-libstdc++" -DCMAKE_C_FLAGS="-D_WIN32_WINNT=_WIN32_WINNT_WIN7" -DCMAKE_CXX_FLAGS="-D_WIN32_WINNT=_WIN32_WINNT_WIN7" $*
